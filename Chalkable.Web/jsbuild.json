{
    "version": "3.0",

    "framework": "./app/bower/emp.ria-framework",

    "libs": {
        "ria": "~/app/bower/emp.ria-framework/"
    },

    "options": {
		"uglifyjs": {
			"output": {
				"beautify": false
			},
			"compress": {
				"dead_code": true
			},
			"mangle": {
			}
		}
	},

    "assets": {
        "jade": { "compiler": "../node_modules/emp.ria-jade/index.js", "options": { "client": true, "compileDebug": false, "self": true } },
        "txt": {},
        "json": {}
    },

    "modules": [{
        "name": "SysAdmin",
        "app": "chlk.SysAdminApp",
        "out": "app/SysAdminApp.compiled.js",

        "prepend": [
            "lib/jade.runtime.js"
        ],

        "globals": [
            "jQuery",
            "jade"
        ]
    }, {
        "name": "TeacherApp",
        "app": "chlk.TeacherApp",
        "out": "app/TeacherApp.compiled.js",

        "prepend": [
            "lib/jade.runtime.js"
        ],

        "globals": [
            "jQuery",
            "jade"
        ]
    }, {
        "name": "DeveloperApp",
        "app": "chlk.DeveloperApp",
        "out": "app/DeveloperApp.compiled.js",

        "prepend": [
            "lib/jade.runtime.js"
        ],

        "globals": [
            "jQuery",
            "jade"
        ]
    }, {
        "name": "AppTesterApp",
        "app": "chlk.AppTesterApp",
        "out": "app/AppTesterApp.compiled.js",

        "prepend": [
            "lib/jade.runtime.js"
        ],

        "globals": [
            "jQuery",
            "jade"
        ]
    },{
        "name": "StudentApp",
        "app": "chlk.StudentApp",
        "out": "app/StudentApp.compiled.js",

        "prepend": [
            "lib/jade.runtime.js"
        ],

        "globals": [
            "jQuery",
            "jade"
        ]
    },{
        "name": "DistrictAdminApp",
        "app": "chlk.DistrictAdminApp",
        "out": "app/DistrictAdminApp.compiled.js",

        "prepend": [
            "lib/jade.runtime.js"
        ],

        "globals": [
            "jQuery",
            "jade"
        ]
    }],

    "dropped": []
}
