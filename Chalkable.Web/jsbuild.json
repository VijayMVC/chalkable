{
    "appDir": "app",

    "options": {
        "uglifyjs": {
            "mangle": false,
            "mangle_options_": {},
            "squeeze": true,
            "squeeze_options_": {
                "make_seqs": true,
                "dead_code": true,
                "no_warnings": false,
                "extra": false
            },
            "beautify": true,
            "separator": ";"
        }
    },

    "modules": [{
        "name": "sysadmin",
        "in": "chlk/SysAdminApp.js",
        "out": "Scripts/SysAdminApp.compiled.js",

        "prepend": [
            "jquery/jquery-2.0.2.min.js",
            "lib/jade.runtime.js"
        ],

        "globals": [
            "jQuery",
            "jade",
            "Highcharts"
        ]
    }]
}