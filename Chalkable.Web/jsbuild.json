{
    "appDir": "app",
    "assetsDir": "assets",

    "options": {
        "uglifyjs": {
            "mangle": false,
            "mangle_options_": {},
            "squeeze": false,
            "squeeze_options_": {
                "make_seqs": true,
                "dead_code": true,
                "no_warnings": false,
                "extra": false
            },
            "beautify": true
        }
    },

    "modules": [{
        "name": "SysAdmin",
        "app": "chlk.SysAdminApp",
        "out": "Scripts/SysAdminApp.compiled.js",

        "prepend": [
            "lib/stacktrace.js",
            "lib/jade.runtime.js"
        ],

        "globals": [
            "jQuery",
            "jade"
        ]
    }, {
        "name": "TeacherApp",
        "app": "chlk.TeacherApp",
        "out": "Scripts/TeacherApp.compiled.js",

        "prepend": [
            "lib/stacktrace.js",
            "lib/jade.runtime.js"
        ],

        "globals": [
            "jQuery",
            "jade"
        ]
    }, {
        "name": "DeveloperApp",
        "app": "chlk.DeveloperApp",
        "out": "Scripts/DeveloperApp.compiled.js",

        "prepend": [
            "lib/stacktrace.js",
            "lib/jade.runtime.js"
        ],

        "globals": [
            "jQuery",
            "jade"
        ]
    }]
}