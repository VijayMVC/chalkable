mixin Grid(controller, action, data, params, noDisplaying)
    .grid(attributes)
        .loader
        .scroller
            .container
                block
    if(controller)
        +Paginator(controller, action, data, params, noDisplaying)

mixin GridHead()
    .head.panel-bg
        block

mixin GridBody()
    .body
        block

mixin GridRow()
    .row
        block