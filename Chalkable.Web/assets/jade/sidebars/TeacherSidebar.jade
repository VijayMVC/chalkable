+ActionLink('feed', 'doToList').inbox(title="View Feed") Feed
+ActionLink('announcement','add').add-new.new-item(title="Add new item") New Item
+ActionLink('attendance', 'summary').attendance(title="View Attendance") Attendance
+ActionLink('grading', 'summary').statistic.grades(title="View Grades") Grades
+ActionLink('discipline', 'list').discipline(title="View Discipline") Discipline
if self.isAppStoreEnabled
    +ActionLink('appmarket', 'list').apps(title="View Apps") Apps
if !self.isLEEnabled || self.isLEEnabled && self.isLinkEnabled
    a(href="/Home/LearningEarnings", target="blank").le(title="View Learning Earnings", target="blank") LE
if self.isLEEnabled && !self.isLinkEnabled
    +ActionLink('error', 'learningEarningsDisabled').le(title="View Learning Earnings", target="blank") LE

if !self.isMessagingDisabled
    +ActionLink('message', 'page', false, true).messages(title="View Messages") Messages
+ActionLink('students', 'my').people(title="View People") People
+ActionLink('calendar', 'month').calendar(title="View Calendar") Calendar
+ActionLink('settings', 'dashboardTeacher').settings(title="View Settings") Settings
