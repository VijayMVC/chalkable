.dialog.chlk-attach-dialog
    .x-window-header
        h1= self.getTitle()
        a.close.close-btn
    .attach-content
        +AttachDialogSidebar(self, self.getApp().getId())
        .attach-main
            iframe(src=self.getUrl(), height="100%", width="100%", border=0, style="border: none; min-height: 500px")
