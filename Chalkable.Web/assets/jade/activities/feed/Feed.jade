.feed-container
    isNotStudent = true
    topData = self.getTopData();
    topItems = topData.getTopItems();
    selectedId = topData.getSelectedItemId();
    +LeftRightToolbar(topItems, chlk.templates.classes.TopBar, 'feed', 'list', [true, self.starredOnly])(
        selectedItemId=selectedId
        ).classes-bar
    .action-bar.not-transparent
        .container.panel-bg
            .i-b
                +ActionLink('feed', 'list', true, !self.starredOnly, selectedId, 0)(class=self.starredOnly ? 'pr' : 'unpr')
                    span#importan-count= self.importantCount
            .feed-notifications Notifications
    +Grid('feed', 'list', self, [true, self.starredOnly, selectedId]).not-transparent
        +GridBody()
            each item in self.items
                div(class=item.isStarred() ? "feed-item starred" : "feed-item")
                    if isNotStudent
                        .grades
                    .bullet
                        +ActionLink('announcement', 'star', item.getId().valueOf(), !item.isStarred()).star
                        +ActionLink('announcement', 'view', item.getId()).d-b.t-d.not-blue
                            .item-type
                                .tooltip-announcement.ann-type
                                    .tooltip-content
                                        Tooltip
                                .attachment
                                    div(class="announcement-type-" + item.getAnnouncementTypeId()).icon.blue
                                    .icon.black
                                    h2.animated-header=item.getTitle()
                                    p.animated=item.getShortContent()
                                    .animated=item.getExpiresDate().toString('d/m/y')
                                    .attachment-icon
                                    .apps-icon