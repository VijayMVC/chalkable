-var clazz = self.getClazz()

.profile-page.class-profile.panorama-page
    h2.page-title=clazz.getName()
    +ProfileTopBar(self, 'panorama')
    .summary-view
        +Tabs.panorama-tabs.stats-container.big
            .top-bar
                UL.tab-headers
                    LI(data-tab=1).tab-header.active Standardized Tests
                    each app, index in clazz.getApps()
                        LI(data-tab=(2 + index)).tab-header=app.getName()
            .panoramas-content
                .tab-content.panorama-content.standardized-tests-cnt(data-tab=1).active.relative
                    +RenderWith(self.getModel(), chlk.templates.classes.ClassProfilePanoramaFormTpl)(
                        userRole=self.getUserRole()
                    )
                each app, index in clazz.getApps()
                    .tab-content.panorama-content(data-tab=(2 + index)).relative
                        .iframe-wrap.partial-update
                            iframe(src=app.getViewUrl(), width="100%", border=0, style="border: none; min-height: 700px", allowfullscreen)





