-var forms = self.getHealthForms()

.chlk-calendar
    .health-forms-grid.chlk-grid
        .grid-content
            .grid-header.grid-row
                .cell.name Form
                .cell.verified Verified
            each form in forms
                +ActionLink('students', 'verifyHealthFormDialog', self.getId(), form.getId()).grid-row.health-form-row
                    .cell.name=form.getName()
                    .cell.verified=form.getVerifiedDate() ? form.getVerifiedDate().getDate().format('m/d/Y h:i A') : '--'