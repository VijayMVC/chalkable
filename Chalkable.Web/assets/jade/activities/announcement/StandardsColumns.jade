standards = self.getItemStandards() || []
if standards.length
    td.name-td
        .standards-names.column
            each standard in standards
                +ActionLink('announcement', 'showStandardsByCategory', self.getClassId(), self.getSubjectId(), self.getId()).standard-name.column-cell
                    .name=standard.getName()
                    .right-triangle
else
    td
        .standards-description=self.getDescription()

