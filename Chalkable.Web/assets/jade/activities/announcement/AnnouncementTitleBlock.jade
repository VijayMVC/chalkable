.title-block
    .title-text=self.getTitle()
    .edit-icon
    .input-container
        INPUT(data-title=self.getTitle(), type="text", name="title", class="validate[required]", value=self.getTitle(), autocomplete="off", maxlength="30")
        div(data-tooltip=Msg.Name_already_taken).exists-icon
    BUTTON(disabled, name="submitType", value="saveTitle", type="submit").validate-skip.blue-gradient-btn.save-title-btn=Msg.Save
    BUTTON(name="submitType", value="checkTitle", type="submit")#check-title-button.validate-skip.x-hidden