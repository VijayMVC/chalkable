.settings
    .action-bar.not-transparent.buttons
        .container.panel-bg
            .items
                +ActionLink('settings', 'dashboardAdmin') Messaging
                each app in self.getApplications()
                    +ActionLink('settings', 'appSettings', app.getId())(class = app.getId() == self.getApp().getId() ? "pressed" : "" )=app.getName()
                if self.isAdminPanoramaEnabled()
                    +ActionLink('settings', 'panoramaAdmin') Panorama
                +ActionLink('settings', 'reportCards') Report Cards

    .iframe-wrap.partial-update
        iframe(src=self.getUrl(), width="100%", border=0, style="border: none; min-height: 700px")
