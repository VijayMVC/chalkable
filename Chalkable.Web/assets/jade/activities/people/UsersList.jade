-users=self.getUsers()
-selectedIndex = self.getSelectedIndex();
.buttons.filter
    .left
        input(type="hidden", name="byLastName", value=(self.isByLastName() ? 'true' : 'false'))
        .buttons-group
            BUTTON(type="button", class=self.isByLastName()?"":"pressed", data-value="false").first-last First name
            BUTTON(type="button", class=self.isByLastName()?"pressed":"", data-value="true").first-last Last name
    .right.wrapper
        input(type="text", placeholder='Filter list...', name="filter", value=self.getFilter()).people-search.search-input
        .search-glass
        .people-search-imgs
            label.people-search-img
            label.people-search-close.opacity0
    .clear-fix

.people-list-container.not-transparent
    .header
        if self.isLEIntegrated()
            .le-link
                +ActionLink('teachers', 'giveLECredits', selectedIndex) Open Learning Earnings
        .title.total-count= self.getTotalText()

    .grid-container.loader-container
        +RenderWith(self.getModel(), chlk.templates.people.UsersGridTpl)(
            userRole = self.getUserRole(),
            currentUser = self.getCurrentUser(),
            rolesText = self.getRoleText(),
            messagingDisabled = self.isMessagingDisabled()
        )



